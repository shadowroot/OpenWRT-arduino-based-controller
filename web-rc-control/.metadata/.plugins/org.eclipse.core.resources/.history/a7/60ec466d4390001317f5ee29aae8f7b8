#include <stdio.h>

#ifndef JSON_H_
#define JSON_H_




struct TObject{

};

struct TArray{
	char ** values;
	int size;
};

union TVALUE{
	char * string;
	int value;
	struct TObject * object;
	struct TArray * array;

};

struct TToken{
	char * param;
	union TVALUE value;
};

#endif /* JSON_H_ */
